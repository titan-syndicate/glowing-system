{
  "name": "glowing-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "i": "npm run interactive",
    "interactive": "npm run list-scripts",
    "lint": "next lint",
    "list-scripts": "node scripts/interactive-npm-scripts.js",
    "start": "next start",
    "sync-secrets": "./scripts/sync-secrets.sh",
    "wf-create-prerelease-pr": "act --secret-file .env.secrets -j create-release-pr --input versionIncrement=prerelease",
    "wf-create-patch-release": "act --secret-file .env.secrets -j create-release-pr --input versionIncrement=patch",
    "wf-update-local-dev-image": "act --secret-file .env.secrets -j update-local-dev-image",
    "wf-helm-upgrade-dev": "act --secret-file .env.secrets -j helm-upgrade-dev",
    "wf-update-helm-repo": "act --secret-file .env.secrets -j update-helm-repo",
    "wf-update-dev-fistbump-chat-image": "act --secret-file .env.secrets -j update-dev-fistbump-chat-image"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.7",
    "@mui/material": "^5.14.7",
    "@types/node": "20.5.1",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "inquirer": "^9.2.10",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "prisma": "^5.2.0"
  }
}
